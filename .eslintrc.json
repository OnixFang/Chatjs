{
    "extends": "airbnb-base",
    "globals": {
        "angular": true,
        "CryptoJS": true
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            "error",
            4
        ],
        "object-shorthand": [
            "error",
            "methods"
        ],
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "no-console": 0
    }
}